<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.multi.bigdataShop.product.comment">
	<select id="newproduct" resultType="productComment"  parameterType="String">
		select *
		from pro_comment
		where prd_no=${prd_no}
	</select>
	<select id="insertComment" resultType="productComment" parameterType="productComment">
		insert into pro_comment
		values ()
	</select>
</mapper>
